doctype html
head
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css')
    title Test
table.table.table-dark
    thead
        tr
            th Image
            th Status
            th Date
    tbody
    each p in people
        tr
            td #{p['image']}
            td #{p['status']}
            td #{p['date']}
